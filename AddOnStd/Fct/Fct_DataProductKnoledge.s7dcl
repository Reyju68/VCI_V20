            Add(
                in1 := #Cmd_DataWrite_Out, 
                in2 := 1, 
                out => #Cmd_DataWrite_Out
            )
            
            Mod(
                in1 := #Cmd_DataWrite_Out, 
                in2 := #Cfg_Buffer_Size, 
                out => #Cfg_Pointer
            )
            
            Add(
                in1 := #Cfg_Pointer, 
                in2 := #Cfg_Buffer_Size, 
                out => #Cfg_Pointer
            )
            
            Mod(
                in1 := #Cfg_Pointer, 
                in2 := #Cfg_Buffer_Size, 
